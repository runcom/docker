{
    "distribution": "FROM centos",
    "dependencies":[
      "  RUN yum -y update && yum install -y \\",
      "  audit-libs-devel \\",
      "  binutils \\",
      "  btrfs-progs-devel \\",
      "  btrfs-progs \\",
      "  bzip2 \\",
      "  gcc \\",
      "  gcc-c++ \\",
      "  glibc-static \\",
      "  glibc-devel \\",
      "  glibc-headers \\",
      "  golang-googlecode-sqlite-devel \\",
      "  kernel-devel \\",
      "  libcap-devel \\",
      "  libvirt-client \\",
      "  libguestfs-tools \\",
      "  make \\",
      "  mingw64-gcc \\",
      "  php-devel \\",
      "  python-websocket-client \\",
      "  python-requests \\",
      "  redhat-rpm-config \\",
      "  rpm-devel \\",
      "  ruby \\",
      "  ruby-devel \\",
      "  tar \\" ],
    "sqlite": [
      "  RUN mkdir -p /usr/src/sqlite3 \\",
      "    && curl -sSL https://www.sqlite.org/2015/sqlite-autoconf-3081002.tar.gz  | tar -v -C /usr/src/sqlite3 -xz",
      "  RUN cd /usr/src/sqlite3/sqlite-autoconf-3081002 \\",
      "    && ./configure --enable-static \\",
      "    && make \\",
      "    && make install \\",
      "    && ldconfig" ],
    "buildtags": "ENV DOCKER_BUILDTAGS selinux",
    "markers": [
      "Cut for distribution specific",
      "End dependencies cut",
      "Sqlite3 install manually",
      "Cut for buildtags distribution",
      "End buildtags cut" ]
}
